{
    "compilerOptions": {
      "target": "es2016",
      "module": "commonjs",
      "outDir": "dist",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "rootDir": "src",
      "experimentalDecorators":true,
      "resolveJsonModule": true,
      "noImplicitAny": true
    },
    "exclude": [
      "src/**/*.spec.ts",
      "src/**/*.test.ts",
      "test/**/*.ts"
    ]
  }
  